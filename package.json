{
  "name": "cyclocomputer-mobile",
  "version": "1.0.0",
  "main": "src/App.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "expo build:android -t apk",
    "build:ios": "expo build:ios -t archive",
    "lint:check:all": "yarn lint:check && yarn format:check",
    "lint": "eslint --ext .js,.ts,.tsx src --fix",
    "lint:check": "eslint --ext .js,.ts,.tsx src",
    "format": "prettier --write src/**/*{.ts,.tsx}",
    "format:check": "prettier --check src/**/*{.ts,.tsx}"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.2",
    "@react-native-community/hooks": "^2.8.1",
    "expo": "~45.0.0",
    "expo-font": "^10.1.0",
    "expo-splash-screen": "^0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.2",
    "material-ui-colors": "^1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-ble-plx": "^2.0.3",
    "react-native-paper": "^4.12.1",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^3.11.0",
    "prettier": "^2.4.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
